@model User;
@{

    List<int> list = new List<int>();
    list = ViewBag.s;


    for (int i = 0; i < 70; i++)
    {
        if (list[i] > 7 && list[i] < 11)
        {
            <a asp-controller="Tisket" asp-action="StartTisket" asp-route-tisid="@(i)" asp-route-queid="@(i * 10 + 1)"><button class="btn btn-outline-success mb-3">@(i + 1)</button></a>
        }
        else if (list[i] > 4 && list[i] < 8)
        {
            <a asp-controller="Tisket" asp-action="StartTisket" asp-route-tisid="@(i)" asp-route-queid="@(i * 10 + 1)"><button class="btn btn-outline-warning mb-3">@(i + 1)</button></a>
        }
        else if (list[i] < 5)
        {
            <a asp-controller="Tisket" asp-action="StartTisket" asp-route-tisid="@(i)" asp-route-queid="@(i * 10 + 1)"><button class="btn btn-outline-danger mb-3">@(i + 1)</button></a>
        }
        else
        {
            <a asp-controller="Tisket" asp-action="StartTisket" asp-route-tisid="@(i)" asp-route-queid="@(i * 10 + 1)"><button class="btn btn-outline-primary mb-3">@(i + 1)</button></a>
        }
        
    }
    <br />



    <a asp-controller="User" asp-action="Obv"><button class="btn btn-outline-success">Update</button></a>
}